<?xml version="1.0" ?>
<beast>

<!-- ntax=4 -->
	<taxa id="taxa">
		<taxon id="A"/>
		<taxon id="B"/>
		<taxon id="C"/>
		<taxon id="D"/>
	</taxa>

<!-- ntax=4 nchar=8000-->
<alignment id="alignment" dataType="nucleotide">
	<sequence>
		<taxon idref="C"/>
		
	</sequence>
	<sequence>
		<taxon idref="A"/>
		
	</sequence>
	<sequence>
		<taxon idref="B"/>
		
	</sequence>
	<sequence>
		<taxon idref="D"/>
		
	</sequence>
</alignment>

	<patterns id="patterns" from="1">
		<alignment idref="alignment"/>
	</patterns>

<newick id="realTree" usingHeights="true" usingDates="false">
	((C:5.0,(A:1.0,B:1.0):4.0):3.0,D:10.0);
</newick>

<newick id="lostTimeTree" usingHeights="true" usingDates="false"> <!-- D is missing 2 units time -->
	((C:5.0,(A:1.0,B:1.0):4.0):3.0,D:8.0);
</newick>

	<treeModel id="realTreeModel">
		<newick idref="realTree"/>
		<rootHeight fixHeights="true">
			<parameter id="realTreeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="realTreeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="realTreeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>

	<treeModel id="lostTimeTreeModel">
		<newick idref="lostTimeTree"/>
		<rootHeight fixHeights="true">
			<parameter id="lostTimeTreeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="lostTimeTreeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="lostTimeTreeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>

	 <arbitraryBranchRates id="realTreeBranchRates" centerAtOne="false">
		<treeModel idref="realTreeModel"/>
		<rates>
            <parameter id="treeBranchRates.rates" value="1E-3 2E-3 3E-3 4E-3 5E-3 6E-3" lower="0"/>
        </rates>
	</arbitraryBranchRates>

	 <arbitraryBranchRates id="lostTimeTreeBranchRates" centerAtOne="false">
		<treeModel idref="lostTimeTreeModel"/>
		<rates>
            <parameter idref="treeBranchRates.rates"/>
        </rates>
	</arbitraryBranchRates>
	
	<timeIncrementBranchRateModel id="correctedLostTimeTreeBranchRates">
		<treeModel idref="lostTimeTreeModel"/>
		<arbitraryBranchRates idref="lostTimeTreeBranchRates"/>
		<taxon idref="D"/>
		<parameter id="offset" value="2.0"/>
	</timeIncrementBranchRateModel>

	<hkyModel id="hky85">
        <frequencies>
            <frequencyModel id='frequencies1' dataType="nucleotide">
                <frequencies>
                   	<parameter id="frequencies" value="0.25 0.25 0.25 0.25"/>
                </frequencies>
            </frequencyModel>
        </frequencies>
        <kappa>
            <parameter id="kappa" value="1.0" lower="0.0"/>
        </kappa>
    </hkyModel>

	<siteModel id="siteModel">
		<substitutionModel>
			<HKYModel idref="hky85"/>
		</substitutionModel>
	</siteModel>

	<treeDataLikelihood id="realTreeLikelihood">
		<patterns idref="patterns"/>
		<treeModel idref="realTreeModel"/>
		<siteModel idref="siteModel"/>
		<arbitraryBranchRates idref="realTreeBranchRates"/>
		<!-- <locationScaledBranchRateModel idref="locationScaledBranchRates"/> -->
	</treeDataLikelihood>
	
	<treeDataLikelihood id="lostTimeTreeLikelihood">
		<patterns idref="patterns"/>
		<treeModel idref="lostTimeTreeModel"/>
		<siteModel idref="siteModel"/>
		<arbitraryBranchRates idref="lostTimeTreeBranchRates"/>		
	</treeDataLikelihood>	

	<treeDataLikelihood id="correctedLostTimeTreeLikelihood">
		<patterns idref="patterns"/>
		<treeModel idref="lostTimeTreeModel"/>
		<siteModel idref="siteModel"/>
		<timeIncrementBranchRateModel idref="correctedLostTimeTreeBranchRates"/>		
	</treeDataLikelihood>	  

	<!-- 
 <branchRateGradient id="substBranchRateGradient" traitName="Sequence">
			<treeDataLikelihood idref="treeLikelihood"/>
	 </branchRateGradient>
 -->

	<report>
		realTree = <treeDataLikelihood idref="realTreeLikelihood"/>
		lostTimeTree = <treeDataLikelihood idref="lostTimeTreeLikelihood"/>
		correctedLosTimeTree = <treeDataLikelihood idref="correctedLostTimeTreeLikelihood"/>
	</report>

    <assertEqual tolerance="1e-3" verbose="true" charactersToStrip="\[\],">
        <message>
            Check log-likelihoods
        </message>
       <actual regex="TreeDataLikelihood\((.*?)\)\n">
	       <treeDataLikelihood idref="correctedLostTimeTreeLikelihood"/>            
        </actual>
        <expected regex="TreeDataLikelihood\((.*?)\)\n">
	         <treeDataLikelihood idref="realTreeLikelihood"/>
        </expected>
    </assertEqual>

</beast>
