    <treeModel id="treeModel">
        <tree idref="startingTree"/>
        <rootHeight>
            <parameter id="treeModel1.rootHeight"/>
        </rootHeight>
        <nodeHeights internalNodes="true">
            <parameter id="treeModel1.internalNodeHeights"/>
        </nodeHeights>
        <nodeHeights internalNodes="true" rootNode="true">
            <parameter id="treeModel1.allInternalNodeHeights"/>
        </nodeHeights>
    </treeModel>

    <!--	INDEPENDENT CTMC -->

    <arbitraryBranchRates id="ind.totalRewardsBranchRateModel" centerAtOne="false">
        <treeModel idref="treeModel"/>
        <rates>
            <parameter id="total.rewards" value="3.75 3 4.5 3"/>
            <!--			<parameter id="ind.total.rewards" dimension="4" lower="0.0"/>-->
        </rates>
    </arbitraryBranchRates>

    <rewardsAwareBranchModel id="ind.rewardModel">
        <!--		<treeModel idref="treeModel"/>-->
        <arbitraryBranchRates idref="ind.totalRewardsBranchRateModel"/>
        <!--		# reward rates = # states ind ctmc-->
        <rewardRates>
            <parameter id="ind.rewardRates" dimension="2" value="2 1" lower="0.0"/>
        </rewardRates>

        <generalSubstitutionModel id="ind.underlying.submodel">
            <generalDataType idref="ind.dataType"/>
            <frequencies>
                <frequencyModel id="ind.frequencyModel" normalize="true">
                    <generalDataType idref="ind.dataType"/>
                    <frequencies>
                        <parameter id="ind.frequencies" dimension="2"/>
                    </frequencies>
                </frequencyModel>
            </frequencies>
            <rates>
                <parameter id="ind.underlying.submodel.rates" dimension="2" value="0.7 0.3"/>
            </rates>
        </generalSubstitutionModel>
    </rewardsAwareBranchModel>

    <report>
        <rewardsAwareBranchModel idref="ind.rewardModel"/>
    </report>

    <assertEqual tolerance="1e-3" verbose="true" charactersToStrip="\[\],">
        <message>
