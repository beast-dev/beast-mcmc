    <siteModel id="ind.siteModel">
        <branchSubstitutionModel>
            <rewardsAwareBranchModel idref="ind.rewardModel"/>
        </branchSubstitutionModel>
    </siteModel>

    <!--&lt;!&ndash;&lt;!&ndash;	do not randomize this! &ndash;&gt;&ndash;&gt;-->
    <strictClockBranchRates id="ind.branchRates">
        <rate>
            <parameter id="ind.clock.rate" value="1" lower="0.0"/>
        </rate>
    </strictClockBranchRates>

    <treeDataLikelihood id="ind.treeLikelihood" useAmbiguities="false" useRewardAwareBranchModelDelegate="true">
        <treeModel idref="treeModel"/>
        <patterns idref="ind.pattern"/>
        <siteModel idref="ind.siteModel"/>
        <rewardsAwareBranchModel idref="ind.rewardModel"/>
        <strictClockBranchRates idref="ind.branchRates"/>
