<?xml version="1.0" standalone="yes"?>
<!-- Testing the "ECM+2kappa" model by Kosiol et al. (2007) -->

<beast>
  <taxa id="taxa">
    <taxon id="HUMAN05790"/>
    <taxon id="MOUSE12058"/>
    <taxon id="CANFA04576"/>
    <taxon id="RATNO13702"/>
