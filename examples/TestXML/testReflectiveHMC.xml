    <!-- The unique patterns for all positions                                   -->
    <!-- npatterns=138                                                           -->
    <patterns id="patterns" from="1">
        <alignment idref="alignment"/>
    </patterns>

    <newick id="tree">
<!--         (((D4Tahiti79:121.64256972309869,D4ElSal83:125.64256972309869):276.15381814384403,(((D4Thai78:5.357290453162822,D4NewCal81:8.357290453162822):73.29206151707947,(D4Brazi82:29.650230415687787,(D4Thai84:4.177729080926932,D4Philip84:4.177729080926932):27.472501334760857):52.999121554554506):80.8685390872532,D4Tahiti85:166.5178910574955):237.27849680944723):383.7475827416806,((D4Thai63:554.3663090231738,(D4PRico86:49.042686290706094,D4Philip56:19.042686290706094):528.3236227324677):158.36915707883077,((D4ElSal94:78.37248218386311,((D4Indon76:5.953824456258175,D4Indon77:6.953824456258175):18.35023582027174,D4SLanka78:26.304060276529917):36.068421907333196):40.57189876319946,(D4Philip64:31.372281359595696,D4Mexico84:51.372281359595696):57.57209958746687):624.791085154942):52.80850450661876); -->
		(((D4Brazi82:1.0, D4ElSal83:2.0):7.0, D4ElSal94:20.0):10.0, D4Indon76:12.0)
    </newick>

	<!-- This is a simple constant population size coalescent model              -->
	<!-- that is used to generate an initial tree for the chain.                 -->
	<constantSize id="constant" units="years">
		<populationSize>
			<parameter id="constant.popSize" value="370.0"/>
		</populationSize>
	</constantSize>


	<treeModel id="treeModel">
		<newick idref="tree"/>
		<rootHeight>
			<parameter id="treeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="treeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
		<nodeTraits name="X" rootNode="false" internalNodes="false" leafNodes="true" traitDimension="2">
			<parameter id="leafTraits"/>
		</nodeTraits>
	</treeModel>

	<coalescentLikelihood id="coalescent">
        <model>
            <constantSize idref="constant"/>
        </model>
        <populationTree>
            <treeModel idref="treeModel"/>
        </populationTree>
    </coalescentLikelihood>

	<!-- Generate a gmrfSkyrideLikelihood for GMRF Bayesian Skyride process      -->
	<gmrfSkyrideLikelihood id="skyride" timeAwareSmoothing="true" randomizeTree="false" intervalNodeMapping="true">
		<populationSizes>

			<!-- skyride.logPopSize is in log units unlike other popSize                 -->
			<parameter id="skyride.logPopSize" dimension="3" value="5.91350300563827"/>
		</populationSizes>
		<groupSizes>
			<parameter id="skyride.groupSize" dimension="3"/>
		</groupSizes>
		<precisionParameter>
			<parameter id="skyride.precision" value="1.0" lower="0.0"/>
		</precisionParameter>
		<populationTree>
			<treeModel idref="treeModel"/>
		</populationTree>
	</gmrfSkyrideLikelihood>

    <arbitraryBranchRates id="branchRates" centerAtOne="false">
        <rates>
            <parameter id="branchRates.rates" value="1" lower="0"/>
        </rates>
        <scale>
            <parameter id="branchRates.scale" lower="0.0" value="1.0"/>
        </scale>
        <location>
            <parameter id="branchRates.location" lower="0.0" value="1E-3"/>
        </location>
        <treeModel idref="treeModel"/>
    </arbitraryBranchRates>

	<rateStatistic id="meanRate" name="meanRate" mode="mean" internal="true" external="true">
		<treeModel idref="treeModel"/>
		<arbitraryBranchRates idref="branchRates"/>
	</rateStatistic>
	<rateStatistic id="coefficientOfVariation" name="coefficientOfVariation" mode="coefficientOfVariation" internal="true" external="false">
		<treeModel idref="treeModel"/>
		<arbitraryBranchRates idref="branchRates"/>
	</rateStatistic>
	<rateCovarianceStatistic id="covariance" name="covariance">
		<treeModel idref="treeModel"/>
		<arbitraryBranchRates idref="branchRates"/>
	</rateCovarianceStatistic>

	<!-- The general time reversible (GTR) substitution model                    -->
	<gtrModel id="gtr">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<alignment idref="alignment"/>
				<frequencies>
					<parameter id="frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<rateAC>
			<parameter id="ac" value="1.0" lower="0.0"/>
		</rateAC>
		<rateAG>
			<parameter id="ag" value="1.0" lower="0.0"/>
		</rateAG>
		<rateAT>
			<parameter id="at" value="1.0" lower="0.0"/>
		</rateAT>
		<rateCG>
			<parameter id="cg" value="1.0" lower="0.0"/>
		</rateCG>
		<rateGT>
			<parameter id="gt" value="1.0" lower="0.0"/>
		</rateGT>
	</gtrModel>


	<!-- site model                                                              -->
	<siteModel id="siteModel">
		<substitutionModel>
			<gtrModel idref="gtr"/>
		</substitutionModel>
		<gammaShape gammaCategories="4">
			<parameter id="siteModel.alpha" value="0.1" lower="0.0"/>
		</gammaShape>
	</siteModel>

	<!-- Likelihood for tree given sequence data                                 -->
	<treeDataLikelihood id="treeLikelihood" useAmbiguities="false" usePreOrder="true"  scalingScheme="dynamic" delayScaling="true">
		<patterns idref="patterns"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="siteModel"/>
		<arbitraryBranchRates idref="branchRates"/>
	</treeDataLikelihood>

    <distributionLikelihood id="ratesPrior">
        <data>
            <parameter idref="branchRates.rates"/>
        </data>
        <distribution>
            <logNormalDistributionModel meanInRealSpace="true">
                <mean>
                    <parameter value="1.0" lower="0.0"/>
                </mean>
                <stdev>
                    <parameter value="1.0" lower="0.0"/>
                </stdev>
            </logNormalDistributionModel>
        </distribution>
    </distributionLikelihood>

	<jointGradient id="jointGradient">
		<hessian>
			<distributionLikelihood idref="ratesPrior"/>
			<parameter idref="branchRates.rates"/>
		</hessian>
		<branchRateGradient id="branchRateGradient" traitName="Sequence" useHessian="true">
			<treeDataLikelihood idref="treeLikelihood"/>
		</branchRateGradient>
	</jointGradient>

	<nodeHeightTransform id="coalescentIntervalTransform">
		<tree idref="treeModel"/>
		<arbitraryBranchRates idref="branchRates"/>
		<nodeHeights>
			<parameter idref="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
		<coalescentIntervals id="coalescent.intervals">
			<gmrfSkyrideLikelihood idref="skyride"/>
		</coalescentIntervals>
	</nodeHeightTransform>

	<gmrfSkyrideGradient id="skyrideGradientWrtInterval" wrtParameter="coalescentInterval">
		<gmrfSkyrideLikelihood idref="skyride"/>
		<parameter idref="coalescent.intervals"/>
		<nodeHeightTransform idref="coalescentIntervalTransform"/>
	</gmrfSkyrideGradient>

	<report>
		<gmrfSkyrideGradient idref="skyrideGradientWrtInterval"/>
	</report>

	<gmrfSkyrideGradient id="skyrideGradientWrtNodeHeight" wrtParameter="nodeHeight">
		<gmrfSkyrideLikelihood idref="skyride"/>
		<parameter idref="treeModel.allInternalNodeHeights"/>
<!-- 		<nodeHeightTransform idref="coalescentIntervalTransform"/> -->
	</gmrfSkyrideGradient>

	<report>
		<gmrfSkyrideGradient idref="skyrideGradientWrtNodeHeight"/>
	</report>

	<graphicalParameterBounds id="graphicalNodeHeightBounds">
		<tree idref="treeModel"/>
		<parameter idref="treeModel.allInternalNodeHeights"/>
	</graphicalParameterBounds>

	<jointGradient id="jointNodeHeightGradient">
		<gmrfSkyrideGradient idref="skyrideGradientWrtNodeHeight"/>
		<nodeHeightGradient id="nodeHeightGradient" traitName="Sequence">
			<treeDataLikelihood idref="treeLikelihood"/>
		</nodeHeightGradient>
	</jointGradient>

	<operators id="operators" optimizationSchedule="log">

        <hamiltonianMonteCarloOperator weight="0.05" nSteps="10" stepSize="1E0" mode="reflective"
                                       drawVariance="1.0" autoOptimize="true">
<!--                                        preconditioning="diagonal" preconditioningDelay="0" preconditioningUpdateFrequency="10"> -->
            <jointGradient idref="jointNodeHeightGradient"/>
            <parameter idref="treeModel.allInternalNodeHeights"/>
            <graphicalParameterBounds idref="graphicalNodeHeightBounds"/>
        </hamiltonianMonteCarloOperator>

	</operators>

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="1000000" autoOptimize="true" fullEvaluation="0">
        <posterior id="posterior">
            <prior id="prior">
                <distributionLikelihood idref="ratesPrior"/>

				<gammaPrior shape="0.05" scale="10.0" offset="0.0">
					<parameter idref="ac"/>
				</gammaPrior>
				<gammaPrior shape="0.05" scale="20.0" offset="0.0">
					<parameter idref="ag"/>
				</gammaPrior>
				<gammaPrior shape="0.05" scale="10.0" offset="0.0">
					<parameter idref="at"/>
				</gammaPrior>
				<gammaPrior shape="0.05" scale="10.0" offset="0.0">
					<parameter idref="cg"/>
				</gammaPrior>
				<gammaPrior shape="0.05" scale="10.0" offset="0.0">
					<parameter idref="gt"/>
				</gammaPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="siteModel.alpha"/>
				</exponentialPrior>

				<gammaPrior shape="0.0010" scale="1000.0" offset="0.0">
					<parameter idref="skyride.precision"/>
				</gammaPrior>

                <exponentialPrior mean="0.3333333333333333" offset="0.0">
                    <parameter idref="branchRates.scale"/>
                </exponentialPrior>
                <exponentialPrior mean="0.3333333333333333" offset="0.0">
                    <parameter idref="branchRates.location"/>
                </exponentialPrior>

                <gmrfSkyrideLikelihood idref="skyride"/>
            </prior>
            <likelihood id="likelihood">
                <treeLikelihood idref="treeLikelihood"/>
            </likelihood>
        </posterior>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="10000">
			<column label="Posterior" dp="4" width="12">
				<posterior idref="posterior"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="10" fileName="testReflectiveHMC.log" overwrite="false">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
            <parameter idref="treeModel.allInternalNodeHeights"/>
			<parameter idref="coalescent.intervals"/>
		</log>
	</mcmc>

	<operators id="operatorsRW" optimizationSchedule="log">

        <randomWalkOperator windowSize="0.05" weight="150.0">
			<parameter idref="treeModel.internalNodeHeights"/>
        </randomWalkOperator>

	</operators>

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmcRW" chainLength="1000000" autoOptimize="true" fullEvaluation="0">
        <posterior idref="posterior"/>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log idref="screenLog"/>

		<!-- write log to file                                                       -->
		<log id="fileLogRW" logEvery="10" fileName="testReflectiveRW.log" overwrite="false">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
            <parameter idref="treeModel.allInternalNodeHeights"/>
			<parameter idref="coalescent.intervals"/>
		</log>
	</mcmc>

	<traceAnalysis fileName="testReflectiveHMC.log" stdError="true"/>
	<traceAnalysis fileName="testReflectiveRW.log" stdError="true"/>



</beast>

