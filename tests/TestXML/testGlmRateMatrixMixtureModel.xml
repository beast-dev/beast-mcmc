	<gtrModel id="gtr">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<frequencies>
					<parameter id="gtr.frequencies" value="0.1 0.2 0.3 0.4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<rates>
			<parameter id="gtr.rates" dimension="6" value="1.0 2.0 1.0 1.0 2.0 1.0" lower="0.0"/>
		</rates>
	</gtrModel>

	<siteModel id="siteModel.GTR">
		<substitutionModel>
			<gtrModel idref="gtr"/>
		</substitutionModel>
	</siteModel>

	<glmSubstitutionModel id="glm.dna" normalize="true" dataType="nucleotide">
		<rootFrequencies>
			<frequencyModel dataType="nucleotide">
				<frequencies>
					<parameter idref="gtr.frequencies"/>
				</frequencies>
			</frequencyModel>
		</rootFrequencies>
		<glmModel family="logLinear" checkIdentifiability="true">
			<independentVariables>
				<parameter id="mixture.coefficient.dna" value="1.0"/>
				<substitutionRateMatrixMixtureModel id="dnamm">
					<gtrModel idref="gtr"/>
				</substitutionRateMatrixMixtureModel>
			</independentVariables>
		</glmModel>
	</glmSubstitutionModel>

	<siteModel id="siteModel.GLM.dna">
		<substitutionModel>
			<glmSubstitutionModel idref="glm.dna"/>
		</substitutionModel>
	</siteModel>

